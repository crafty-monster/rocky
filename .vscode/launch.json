{
  "version": "0.2.0",
  "configurations": [
    {
      "address": "TCP/IP address of process to be debugged",
      "localRoot": "${workspaceFolder}",
      "name": "Attach to Remote",
      "port": 9229,
      "remoteRoot": "Absolute path to the remote directory containing the program",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Inspect",
      "request": "launch",
      "type": "node-terminal",
      "command": "npm run inspect"
    }
  ]
}